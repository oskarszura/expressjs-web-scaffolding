<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width">
  <title><%-title %></title>
  <link rel="stylesheet" href="/styles.css">
  <script src="http://localhost:35729/livereload.js"></script>
</head>
<body data-user-id="<%-userId %>">
 <header class="t-header">
     <a class="t-header__logo" href="/" id="root">
        LOGO
     </a>
     <nav class="t-header__navigation">
       <ul class="t-header__navigation-list">
         <% if (!logged) { %>
         <li class="t-header__navigation-option"
             role="presentation">
           <a href="/login">
             Login
           </a>
         </li>
         <li class="t-header__navigation-option"
             role="presentation">
           <a href="/login/register">
             Sign up
           </a>
         </li>
         <% } %>
         <% if (logged) { %>
         <li class="t-header__navigation-option"
             role="presentation">
           <a href="/admin">
             Admin
           </a>
         </li>
         <li class="t-header__navigation-option"
             role="presentation">
           <a href="/login/logout">
             Logout
           </a>
         </li>
         <% } %>
       </ul>
     </nav>
 </header>
